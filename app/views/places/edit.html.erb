
<h1>Edit Places</h1>
<%= form_for :place, url: places_path do |f| %>
  

  <p>
    <%= f.label :place_name %><br>
    <%= f.text_field :place_name %>
  </p>

  <p>
    <%= f.label :node_name %><br>
    <%= f.text_field :node_name %>
  </p>



  <p>
    <%= f.collection_select :company_id, Company.all, :id, :name, prompt: true %>
    </p>


  <p>
    <%= f.submit %>
  </p>
<% end %>
<%= link_to 'Back', places_path %>