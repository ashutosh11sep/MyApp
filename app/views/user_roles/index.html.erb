<h1>Listing User_roles</h1>
 <%= link_to 'New user_role', new_user_role_path %>
<table>
  <tr>
    <th>Role_name</th>
  </tr>
 
  <% @user_roles.each do |user_role| %>
    <tr>
      <td><%= user_role.role_name  %></td>
      <td><%= link_to 'Show', user_role_path(user_role) %></td>
      <td><%= link_to 'Edit', edit_user_role_path(user_role) %></td>
      <td><%= link_to 'Destroy', user_role_path(user_role),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>

    </tr>
  <% end %>
</table>
<%= link_to 'Back', home_index_path %>
