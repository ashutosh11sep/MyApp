<h1>Edit Images</h1>
<%= form_for :image, url: images_path do |f| %>
  <p>
    <%= f.label :user_id %><br>
    <%= f.text_field :user_id %>
  </p>
<p>
    <%= f.label :real_name %><br>
    <%= f.text_field :real_name %>
  </p>
  <p>
    <%= f.label :image_name %><br>
    <%= f.text_field :image_name %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
<%= link_to 'Back', images_path %>